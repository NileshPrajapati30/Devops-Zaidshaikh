pipeline {
    agent any
    enviroment {
        PATH = "PATH:/opt/apahe-maen /bin"
    stages {
        stage('Clone sources') {
            steps {
                git url: 'https://github.com/saamrajepatil/hello-world.git'
            }
        }
        stage("build & SonarQube analysis") {
            agent any
            steps {
              withSonarQubeEnv('sonarqube-8') {
                sh 'mvn clean package sonar:sonar'
              }
            }
          }
